# Hotel Booking API Configuration

# Database settings
database:
  name: "hotel_booking"
  user: "postgres"
  password: "postgres"
  host: "localhost"
  port: 5433

# Django settings
django:
  debug: true
  secret_key: "your-secret-key-change-in-production"
  allowed_hosts: ["*"]

# API settings
api:
  max_page_size: 100
  default_page_size: 20

# Logging
logging:
  level: "INFO"
  format: "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
